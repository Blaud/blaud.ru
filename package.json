{
  "name": "blaud.ru",
  "version": "1.0.0",
  "description": "my cv website",
  "main": "index.js",
  "engines": {
    "node": "8.9.4",
    "npm": "6.3.0"
  },
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blaud/blaud.ru.git"
  },
  "keywords": [
    "Resume",
    "Blaud",
    "Express",
    "Angular"
  ],
  "author": "Nikolay Dvoeglazov (Blaud)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Blaud/blaud.ru/issues"
  },
  "homepage": "https://github.com/Blaud/blaud.ru#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.7",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
